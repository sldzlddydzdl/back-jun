import java.util.Scanner;
public class Main{

    public static void main(String[] args){

      Scanner sc = new Scanner(System.in);
      
      int N = sc.nextInt();
      
      System.out.println(factor(N));
      
    }
    
    public static int factor(int N){
      
      if(N == 0){
        return 1;
      }
      
      else{
        return N*factor(N-1);
      }
    }
}
